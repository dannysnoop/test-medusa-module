{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "guest-cart-medusa",
  "version": "3.0.0",
  "description": "plugin guest cart.",
  "author": "Medusa (https://medusajs.com)",
  "license": "UNLICENSED",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "format": "prettier . --write",
    "lint": "eslint --ext .js,.ts .",
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && npm run build:server",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "medusa": "cross-env medusa",
    "seed": "cross-env medusa seed -f ./data/seed.json",
    "start": "cross-env npm run build && medusa start",
    "migrate-and-start": "medusa migrations run && medusa start",
    "start:custom": "cross-env npm run build && node --preserve-symlinks --trace-warnings index.js",
    "dev": "cross-env npm run build:server && medusa develop",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "apidoc:store": "medusa-oas oas --type store --paths ./src/api/store ./src/models",
    "apidoc:admin": "medusa-oas oas --type admin --paths ./src/api/admin ./src/models ./src/api/webhooks",
    "prepare": "husky"
  },
  "dependencies": {
    "@medusajs/cache-inmemory": "^1.8.11",
    "@medusajs/cache-redis": "^1.9.2",
    "@medusajs/event-bus-local": "^1.9.9",
    "@medusajs/event-bus-redis": "^1.8.14",
    "@medusajs/file-local": "^1.0.4",
    "@medusajs/icons": "^1.2.2",
    "@medusajs/inventory": "^1.11.7",
    "@medusajs/medusa": "1.20.9",
    "@medusajs/modules-sdk": "^1.12.12",
    "@medusajs/stock-location": "^1.11.6",
    "@medusajs/types": "^1.11.17",
    "@medusajs/ui": "^2.4.3",
    "@medusajs/utils": "^1.11.10",
    "@perseidesjs/medusa-plugin-rate-limit": "^1.0.8",
    "@rsc-labs/medusa-products-bought-together": "^0.1.4",
    "@rsc-labs/medusa-store-analytics": "^0.12.2",
    "@types/multer": "^1.4.11",
    "awesome-phonenumber": "^7.2.0",
    "awilix": "^8.0.0",
    "aws-sdk": "^2.1691.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compare-versions": "^6.1.1",
    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.6.0",
    "google-auth-library": "^9.14.1",

    "jose": "^5.8.0",
    "logform": "^2.6.0",
    "medusa-core-utils": "^1.2.3",
    "medusa-file-s3": "^1.4.2",
    "medusa-fulfillment-manual": "^1.1.41",
    "medusa-interfaces": "^1.3.10",
    "medusa-payment-manual": "^1.0.25",
    "medusa-payment-paypal": "^6.0.5",
    "medusa-payment-stripe": "^6.0.11",
    "medusa-plugin-algolia": "^0.2.21",
    "medusa-plugin-sendgrid": "^1.3.13",
    "medusa-plugin-sentry": "^1.5.0",
    "medusa-plugin-twilio-sms": "^1.2.9",
    "openai": "^4.83.0",
    "ora": "^5.4.1",
    "papaparse": "^5.4.1",
    "phone": "^3.1.50",
    "querystring": "^0.2.1",
    "stripe": "^15.12.0",
    "typeorm": "^0.3.20",
    "ulid": "^2.3.0",
    "winston": "^3.13.0",
    "winston-transport-sentry-node": "^3.0.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@medusajs/admin": "^7.1.12",
    "@medusajs/medusa-cli": "^1.3.23",
    "@medusajs/medusa-oas-cli": "0.3.1",
    "@tsconfig/node18": "^18.2.2",
    "@tsconfig/recommended": "^1.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/papaparse": "^5.3.14",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.spec.json"
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
